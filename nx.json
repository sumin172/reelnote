{
  "installation": {
    "version": "22.0.3"
  },
  "projects": {
    "review-service": "reelnote-api/review-service",
    "catalog-service": "reelnote-api/catalog-service",
    "frontend": "reelnote-frontend",
    "e2e-frontend": "tests/e2e-frontend",
    "e2e-catalog": "tests/e2e-catalog",
    "e2e-review": "tests/e2e-review",
    "e2e-cross": "tests/e2e-cross",
    "e2e-env": "tests/e2e-env",
    "api-schema": "packages/api-schema",
    "contract-frontend-catalog": "tests/contract-frontend-catalog"
  },
  "plugins": [
    {
      "plugin": "@nx/webpack/plugin",
      "options": {
        "buildTargetName": "build",
        "serveTargetName": "serve",
        "previewTargetName": "preview",
        "buildDepsTargetName": "build-deps",
        "watchDepsTargetName": "watch-deps",
        "serveStaticTargetName": "serve-static"
      }
    },
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "eslint:lint"
      }
    },
    {
      "plugin": "@nx/jest/plugin",
      "options": {
        "targetName": "test",
        "disableJestRuntime": true
      },
      "exclude": [
        "tests/e2e-catalog/**/*"
      ]
    }
  ],
  "targetDefaults": {
    "test": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}
