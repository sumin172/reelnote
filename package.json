{
  "name": "reelnote",
  "version": "0.0.1",
  "private": true,
  "description": "ReelNote - Movie review platform with microservices",
  "packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  },
  "pnpm": {
    "overrides": {
      "glob": "^10.4.5"
    },
    "ignoredBuiltDependencies": [
      "@nestjs/core",
      "@parcel/watcher",
      "@prisma/client",
      "@prisma/engines",
      "@scarf/scarf",
      "@swc/core",
      "@tailwindcss/oxide",
      "esbuild",
      "msw",
      "prisma",
      "sharp",
      "unrs-resolver"
    ]
  },
  "scripts": {
    "build": "nx run-many --target=build --all",
    "test": "nx run-many --target=test --all --output-style=stream",
    "dev": "nx run-many --target=serve --projects=catalog-service,frontend,review-service --parallel --maxParallel=3 --output-style=stream",
    "affected:build": "nx affected --target=build",
    "affected:test": "nx affected --target=test",
    "graph": "nx graph",
    "prepare": "husky",
    "postinstall": "nx daemon --start",
    "daemon:start": "nx daemon --start",
    "daemon:stop": "nx daemon --stop",
    "api-schema:generate": "node tools/scripts/generate-api-schema.mjs",
    "api-schema:clean": "node tools/scripts/generate-api-schema.mjs --clean",
    "clean:build": "pwsh -NoProfile -ExecutionPolicy Bypass -File ./scripts/clean-build-outputs.ps1 -ForceRebuild",
    "clean:build:dry": "pwsh -NoProfile -ExecutionPolicy Bypass -File ./scripts/clean-build-outputs.ps1 -DryRun",
    "clean:cache": "nx reset && pwsh -NoProfile -ExecutionPolicy Bypass -File ./scripts/clean-workspace-cache.ps1 -SkipBuildCache",
    "clean:ide": "pwsh -NoProfile -ExecutionPolicy Bypass -File ./scripts/clean-workspace-cache.ps1 -SkipBuildCache",
    "clean:all": "pwsh -NoProfile -ExecutionPolicy Bypass -File ./scripts/clean-build-outputs.ps1 -ForceRebuild && nx reset && pwsh -NoProfile -ExecutionPolicy Bypass -File ./scripts/clean-workspace-cache.ps1 -SkipBuildCache",
    "lint:kotlin": "pnpm nx run review-service:lint",
    "lint:ts": "pnpm nx run-many --target=lint --projects=catalog-service,frontend",
    "lint:e2e-ts": "pnpm nx run-many --target=lint --projects=e2e-catalog,e2e-frontend",
    "lint:e2e-kotlin": "pnpm nx run e2e-review:lint",
    "format:kotlin": "pnpm nx run review-service:ktlintFormat",
    "format:ts": "pnpm nx run-many --target=format:write --projects=catalog-service,frontend",
    "format:ts:check": "pnpm nx run-many --target=format:check --projects=catalog-service,frontend",
    "format:e2e-ts": "pnpm nx run-many --target=lint-fix --projects=e2e-catalog,e2e-frontend",
    "format:e2e-kotlin": "pnpm nx run e2e-review:lint-fix",
    "up:all": "pnpm nx run e2e-env:start:docker",
    "up:db": "pnpm nx run e2e-env:start:docker:db",
    "down": "pnpm nx run e2e-env:stop:docker",
    "e2e:local:start": "pnpm nx run e2e-env:start:local",
    "e2e:local:stop": "pnpm nx run e2e-env:stop:local"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.8",
    "@nx/eslint": "^22.0.3",
    "@nx/eslint-plugin": "^22.0.3",
    "@nx/gradle": "^22.0.3",
    "@nx/jest": "^22.0.3",
    "@nx/js": "^22.0.3",
    "@nx/nest": "^22.0.3",
    "@nx/next": "^22.0.3",
    "@nx/node": "^22.0.3",
    "@nx/web": "^22.0.3",
    "@nx/webpack": "^22.0.3",
    "@playwright/test": "^1.56.1",
    "@swc-node/register": "~1.11.1",
    "@swc/cli": "~0.7.9",
    "@swc/core": "~1.15.1",
    "@swc/helpers": "~0.5.17",
    "@swc/jest": "~0.2.39",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "chalk": "^5.6.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-node": "^30.2.0",
    "jest-util": "^30.2.0",
    "lines-and-columns": "^2.0.4",
    "nx": "22.0.3",
    "prettier": "^3.6.2",
    "prisma": "^6.19.0",
    "ts-node": "10.9.2",
    "tslib": "^2.8.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/swagger": "^11.2.1",
    "@prisma/client": "^6.19.0",
    "axios": "^1.13.2",
    "axios-retry": "^4.5.0",
    "cache-manager": "^7.2.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "ioredis": "^5.8.2",
    "opossum": "^9.0.0",
    "p-limit": "^7.2.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  }
}
